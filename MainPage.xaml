<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
  x:Class="shinypushbug.MainPage"
  xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
  xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
  xmlns:shinypushbug="clr-namespace:shinypushbug"
  x:DataType="shinypushbug:MainViewModel">

  <VerticalStackLayout>
    <Label Text="{Binding FcmToken}" />

    <Button
      Command="{Binding ToggleRegistrationCommand}"
      Text="Register/UnRegister" />
  </VerticalStackLayout>

</ContentPage>
